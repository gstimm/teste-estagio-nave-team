{
  "name": "teste-estagio-back-end-naveteam",
  "version": "0.0.1",
  "description": "A Node API made for Nave Team internship test.",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec babel-node src/server.js",
    "build": "rm -rf build && mkdir build && babel src -s -d build",
    "knex": "babel-node node_modules/.bin/knex --knexfile knexfile.js",
    "db:migrate": "yarn knex migrate:latest",
    "db:seed": "yarn knex seed:run"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "knex": "^0.95.1",
    "objection": "^2.2.14",
    "pg": "^8.5.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/preset-env": "^7.13.9",
    "nodemon": "^2.0.7"
  }
}
